
@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>
<br />

<div class="form-group">
    <label for="username">Username:</label>
    <input type="text" class="form-control" placeholder="Please enter a username" id="username" />
</div>
<div class="form-group">
    <label for="password">Password:</label>
    <input type="text" class="form-control" placeholder="Please enter a password" id="password" />
</div>
<br />
<button id="registerButton" type="submit" class="btn btn-primary">Login!</button>

